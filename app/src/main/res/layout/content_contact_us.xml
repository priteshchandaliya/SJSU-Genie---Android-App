<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:background="#E6E6E6"
    android:paddingTop="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.sjsu.priteshchandaliya.cmpeginie.contactUS"
    tools:showIn="@layout/app_bar_contact_us">

    <ImageView
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:id="@+id/pritesh"
        android:src="@drawable/pritesh"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:layout_alignParentRight="true"
        android:paddingLeft="95dp"
        android:id="@+id/priteshtext"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <ImageView
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:id="@+id/megha"
        android:src="@drawable/megha"
        android:layout_below="@+id/pritesh"
        android:layout_alignParentStart="true"
        android:layout_marginTop="50dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:layout_alignParentRight="true"
        android:paddingLeft="95dp"
        android:id="@+id/meghatext"
        android:layout_alignTop="@+id/megha"
        android:layout_alignStart="@+id/priteshtext" />
    <ImageView
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:id="@+id/aniket"
        android:src="@drawable/aniket"
        android:layout_below="@+id/megha"
        android:layout_alignParentStart="true"
        android:layout_marginTop="50dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:layout_alignParentRight="true"
        android:paddingLeft="95dp"
        android:id="@+id/anikettext"
        android:layout_alignTop="@+id/aniket"
        android:layout_alignStart="@+id/meghatext" />

    <ImageView
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:id="@+id/nirmit"
        android:src="@drawable/nirmit"
        android:layout_marginTop="50dp"
        android:layout_below="@+id/aniket"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:layout_alignParentRight="true"
        android:paddingLeft="95dp"
        android:id="@+id/nirmittext"
        android:layout_alignTop="@+id/nirmit"
        android:layout_alignStart="@+id/anikettext" />




</RelativeLayout>

